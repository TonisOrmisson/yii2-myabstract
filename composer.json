{
  "name": "andmemasin/yii2-myabstract",
  "license":"proprietary",

  "type": "yii2-extension",

  "description": "my custom activerecord classes",
  "keywords": ["yii2", "andmemasin"],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }

  ],
  "authors": [
    {
      "name": "TÃµnis Ormisson",
      "email": "tonis@andmemasin.eu"
    }
  ],

  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1.2",
    "yiisoft/yii2": "^2.0.14",
    "andmemasin/yii2-helpers": "^1.5.0"
  },
  "require-dev": {

    "codeception/codeception": "^5.0",
    "codeception/module-filesystem": "^3.0.0",
    "codeception/module-yii2": "^1.1.7",
    "codeception/module-asserts": "^3.0.0",
    "codeception/module-db": "^3.0",

    "phpstan/phpstan": "^1"
  },
  "config": {
    "platform": {
      "php": "8.1.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "8.2.x-dev"
    }
  },

  "autoload": {
    "psr-4": {
      "andmemasin\\myabstract\\": "src"
    }
  }

}
