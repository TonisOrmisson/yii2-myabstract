{
  "name": "andmemasin/yii2-myabstract",
  "license":"proprietary",

  "type": "yii2-extension",

  "description": "my custom activerecord classes",
  "keywords": ["yii2", "andmemasin"],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }

  ],
  "authors": [
    {
      "name": "TÃµnis Ormisson",
      "email": "tonis@andmemasin.eu"
    }
  ],

  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0.2",
    "yiisoft/yii2": "^2.0.14",
    "andmemasin/yii2-helpers": "^1.5.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/specify": "~0.4.6",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "codeception/module-filesystem": "^1.0.0",
    "codeception/module-yii2": "^1.1.0",
    "codeception/module-asserts": "^1.3.0",
    "codeception/module-db": "^1.0",
    "phpstan/phpstan": "^1.8"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "5.0.x-dev"
    }
  },

  "autoload": {
    "psr-4": {
      "andmemasin\\myabstract\\": "src"
    }
  }

}
